<h2>View all Directors</h2>

<% directors.forEach(director => { %>
    <div> <%= director.firstName %> <%= director.lastName %> </div>
    <a href="/directors/<%= director.id %>">View</a>
    <a href="/directors/<%= director.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm',{url:`/directors/${director.id}`}) %> 
<% }) %>